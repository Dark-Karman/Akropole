<Window x:Class="Akropole.MenuRentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Akropole"
        mc:Ignorable="d"
        Title="MenuRentWindow" Height="auto" Width="auto">
    <Grid>
        <Image Source="/Resource/453_original.jpg" />

        <StackPanel VerticalAlignment="Center">
            <Button x:Name="overwatchBtn" Margin="10" Click="overwatchBtn_Click" Content="Просмотр"/>
            <Button x:Name="otchetBtn" Click="otchetBtn_Click" Content="Отчёт" Margin="10"/>
            <Button Content="Выйти" Name="exitBtn" Click="exitBtn_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
